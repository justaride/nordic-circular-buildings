  "lessons_learned": {
    "challenges": [
      {
        "issue": "Skjult asbest og ukjent infrastruktur",
        "impact": "Riving tok dobbelt så lang tid som planlagt (3-4 mnd → 6-8 mnd)",
        "resolution": "Omfattende replanning underveis"
      },
      {
        "issue": "Ingen riggplass utenfor bygget",
        "impact": "Logistiske utfordringer",
        "resolution": "Nattleveranser og vindusheiser for materialinntak"
      },
      {
        "issue": "Begrenset lagringsplass",
        "impact": "Materialhåndtering krevende",
        "resolution": "Just-in-time leveranser essensielt"
      },
      {
        "issue": "Kommunikasjonsproblemer med ombrukt teknisk utstyr",
        "impact": "Utfordringer med automatikksystemer",
        "resolution": "Burde ha testet kommunikasjon/samhandling tidligere",
        "recommendation": "Ville kanskje ikke ombrukt så mye teknisk utstyr igjen"
      },
      {
        "issue": "Dårlig merking av komponenter",
        "impact": "Flere komponenter ble revet (undersentraler, pumper, energimålere)",
        "resolution": "Tydeligere merking av hva som skal ombrukes"
      },
      {
        "issue": "QR-koder fjernet av leietakere",
        "impact": "Noen QR-koder ble plukket bort",
        "resolution": "Bedre informasjon til leietakere om QR-kodene"
      },
      {
        "issue": "Ekstern ombruk av flis tidkrevende",
        "impact": "Byggherre brukte mye tid på koordinering",
        "resolution": "Avsett egen pott til ombruk og egen ressurs til oppfølging"
      }
    ],
    "success_factors": [
      "Tidlig ombrukskartlegging ga beslutningsgrunnlag",
