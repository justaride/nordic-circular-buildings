    "lessons_learned_plastic": [
      {
        "category": "Målkonflikt",
        "finding": "Plastreduksjon og klimagassreduksjon går ikke alltid hånd i hånd",
        "detail": "Flere materialvalg for plastreduksjon førte til økt CO2-utslipp (f.eks. steinull vs EPS)"
      },
      {
        "category": "FutureBuilt-krav",
        "finding": "50%-kravet kan virke mot sin hensikt",
        "detail": "Kan oppmuntre til å beholde plast (biobasert/resirkulert) istedenfor å erstatte helt"
      },
      {
        "category": "Demonterbarhet",
        "finding": "95% demonterbarhetskrav vanskelig for flere materialer",
        "detail": "Isolasjon (80%), sparkel/maling (0%), membraner (svært vanskelig)"
      },
      {
        "category": "Våtrom",
        "finding": "Tekniske krav krever polymerinnhold",
        "detail": "Membran, flislim og våtromsmaling må inneholde polymer for godkjenning"
      },
      {
        "category": "Yttertak",
        "finding": "Ingen plastfrie alternativer funnet",
        "detail": "Dampsperrer, membraner og tettematerialer krever plast for funksjon"
      }
    ],
    "recommendations_to_futurebuilt": [
      "Revurdere 50%-kravet slik at full plastfjerning premieres, ikke bare biobasert erstatning",
